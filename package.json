{
  "name": "lit-it",
  "displayName": "lit-it",
  "description": "lazy js doc builder",
  "version": "1.0.1",
  "publisher": "mohseenrm",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.litIt"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.litIt",
        "title": "Lit it!",
        "description": "Add JSdocs for your JS code"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "chai": "^3.5.0",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
